
@{
    ViewData["Title"] = "Index";
}

@model IEnumerable<CustomerTypes>

<div class="border whitebackgroundBorder" style="padding:100px">
    <div class="row">
        <div class="col-6 text-info">CustomerType</div>
        <div class="col-6 text-end">Create</div>
    </div>
    @if (Model.Count() > 0)
    {
        <table class="table table-striped border">
            <thead>
                <tr class="table-secondary">
                    <th>@Html.DisplayNameFor(item => item.CustomerType)</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in @Model)
                 {
                    <tr>
                        <td>@item.CustomerType</td>
                    </tr>
                 }

            </tbody>
        </table>
    }
</div>







